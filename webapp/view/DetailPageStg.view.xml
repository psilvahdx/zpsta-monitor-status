						<OverflowToolbar id="_IDGenOverflowToolbar1">
							<content>
								<Title id="_IDGenTitle1" text='Registros com status {ModelIconTabFilterStg>/status}'/>
							</content>
						</OverflowToolbar>
					</headerToolbar>
					<infoToolbar>
						<OverflowToolbar id="_IDGenOverflowToolbar2">
							<Label id="_IDGenLabel1" text="Para mais detalhes, consulte os registros nas tabelas utilzando os ícones no topo da página"/>
						</OverflowToolbar>
					</infoToolbar>
					<columns>
						<Column id="_IDGenColumn1"
						width="5rem">
							<Text id="_IDGenText1" text="Codigo Conector"/>
						</Column>
						<Column id="_IDGenColumn4" 
						width="5rem">
							<Text id="_IDGenText7"  text="Descrição"/>
						</Column>
						<Column id="_IDGenColumn2"
						width="5rem">
							<Text id="_IDGenText2" text="Tabela"/>
						</Column>
						<Column id="_IDGenColumn3"
						width="10rem">
							<Text id="_IDGenText3" text="Total"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem id="_IDGenColumnListItem1">
							<cells>
								<Text id="_IDGenText4" text="{CodigoConector}"/>
								<Text id="_IDGenText8" text="{Descricao}"/>
								<Text id="_IDGenText5" text="{Tabela}"/>
								<Text id="_IDGenText6" text="{ path: 'TotalRegistros', formatter: '.formatter.numberFormat' }"/>
							</cells>
						</ColumnListItem>
					</items>
					</Table>	
				</content>
			</IconTabFilter>
			<IconTabSeparator id="_IDGenIconTabSeparator1"/>
			<IconTabFilter id="_IDGenIconTabFilter2"
				icon="sap-icon://competitor"
